public void add_Molecule_Rectangle () {
  final double epsilon = 0.06;	// small distance
  double alat=Math.pow(2.0,(1/6))*1.13;
  rect_unit_cellx=alat*Math.sqrt(3);
  double rect_unit_celly=alat;
  double basis_atom1_x=0;
  double basis_atom1_y=0;
  double basis_atom2_x=0.5*rect_unit_cellx;
  double basis_atom2_y=0.5*rect_unit_celly;
  int i, j;
  int m=0;

  double middle=boxWidth/2.0;
  //  double cornerx=middle-Ncells_x/2*rect_unit_cellx;
  //double cornery=middle-Ncells_y/2*rect_unit_celly;
  double cornerx=sigma_AA*1.1-0.5;
  double cornery=sigma_AA*1.1-0.1;
  initial_length=(Ncells_x-1)*rect_unit_cellx;

  for (i=0;i<Ncells_y;i++) {
    for (j=0;j<Ncells_x;j++) {
      atom_type[m]=1;
      atom_type[m+1]=1;
      x[m] = cornerx+j*rect_unit_cellx+basis_atom1_x+(Math.random()-.5)*epsilon;
      y[m] = cornery+i*rect_unit_celly+basis_atom1_y+(Math.random()-.5)*epsilon;
      x[m+1] = cornerx+j*rect_unit_cellx+basis_atom2_x+(Math.random()-.5)*epsilon;
      y[m+1] = cornery+i*rect_unit_celly+basis_atom2_y+(Math.random()-.5)*epsilon;

      vx[m] = 0; vy[m] = 0;
      ax[m] = 0; ay[m] = 0;
      vx[m+1] = 0; vy[m+1] = 0;
      ax[m+1] = 0; ay[m+1] = 0;
//      atom_type2[m]=0;
  //    atom_type2[m+1]=0;

//      if (j==0) {
  //      atom_type2[m]=1;
    //    atom_type2[m+1]=1;
        //    vx[m]=-1*strain_rate*Ncells_x*rect_unit_cellx/2.0;
        //    vx[m+1]=-1*strain_rate*Ncells_x*rect_unit_cellx/2.0;
        //
//      }
//      if (j==Ncells_x-1) {
  //      atom_type2[m]=2;
    //    atom_type2[m+1]=2;
        //    vx[m]=strain_rate*Ncells_x*rect_unit_cellx/2.0;
        //    vx[m+1]=strain_rate*Ncells_x*rect_unit_cellx/2.0;
        //
      //}
      m=m+2;

    }
  }
}
]]></Code>
