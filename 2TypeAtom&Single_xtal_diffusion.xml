<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void computeAccelerations() {
  double dx, dy; // separations in x and y directions
  double dx2, dy2, rSquared, rSquaredInv, attract, repel, fOverR=1.0, fx, fy;

  pe = 0; // compute the potential energy
  double wallForce = 0.0; // and the pressure

  // first check for bounces off walls, and include gravity (if any):
  for (int i = 0; i < N; i++) {
    if (x[i] < 0.5) {
      ax[i] = wallStiffness * (0.5 - x[i]);
      wallForce += ax[i];
      pe += 0.5* wallStiffness * (0.5 - x[i]) * (0.5 - x[i]);
    } else if (x[i] > (boxWidth - 0.5)) {
      ax[i] = wallStiffness * (boxWidth - 0.5 - x[i]);
      wallForce -= ax[i];
      pe += 0.5 * wallStiffness * (boxWidth - 0.5 - x[i])*(boxWidth - 0.5 - x[i]);
    } else
    ax[i] = 0.0;
    if (y[i] < 0.5) {
      ay[i] = (wallStiffness * (0.5- y[i]));
      wallForce += ay[i];
      pe += 0.5 * wallStiffness * (0.5 - y[i]) * (0.5 - y[i]);
    } else if (y[i] > (boxWidth - 0.5)) {
      ay[i] = (wallStiffness * (boxWidth - 0.5 - y[i]));
      wallForce -= ay[i];
      pe += 0.01 * wallStiffness * (boxWidth - 0.01 - y[i])
      * (boxWidth - 0.01 - y[i]);
    } else{
      ay[i] = 0;
    }
//    if (atom_type[i] == 0)  
  //  {
    //  ax[i] -= Ex * Math.cos(omega_x * t + phase_x);
      //ay[i] -= Ey * Math.cos(omega_y * t);//  acceleration of gravity
//    }
  //  else
    //{
//      ax[i] += Ex * Math.cos(omega_x * t + phase_x);
  //    ay[i] += Ey * Math.cos(omega_y * t);
    //}
    ay[i] -= gravity;
  }
  pressure = wallForce / (4 * boxWidth);

  // Now compute interaction forces (Lennard-Jones potential).
  // This is where the program spends most of its time (when N is large),
  // so avoid unnecessary calculations and array lookups.
  for (int i = 0; i < N; i++) 
  {
    for (int j = 0; j < i; j++) 
    {
      // loop over all distinct pairs
      dx = x[i] - x[j];
      dx2 = dx * dx;
      if (dx2 < forceCutoff2_array[i][j]) 
      {
        // close enough to bother
        dy = y[i] - y[j];
        dy2 = dy * dy;
        if (dy2 < forceCutoff2_array[i][j]) 
        {
          rSquared = dx2 + dy2;
          if (rSquared < forceCutoff2_array[i][j]) 
          {
            rSquaredInv = forceCutoff2_array[i][j]/forceCutoff2 / rSquared;
//            attract = 1/Math.sqrt(rSquaredInv);
            attract=rSquaredInv*rSquaredInv*rSquaredInv;
            repel = rSquaredInv * rSquaredInv * rSquaredInv * rSquaredInv * rSquaredInv * rSquaredInv;
            if (atom_type[i]==0 && atom_type[j]==0) 
            {
          //    pe += epsilon_AA * 4.0 * (repel + attract) - pEatCutoff_AA;
          //    fOverR = epsilon_AA * ((48.0 * repel) + attract)/rSquared;
              pe += epsilon_AA*((4.0 * (repel - attract)) - pEatCutoff);
              fOverR = epsilon_AA*24.0 * ((2.0 * repel) - attract)/rSquared;
           }
            else if (atom_type[i]==1 && atom_type[j]==1) 
            {
            //  pe += epsilon_BB * 4.0 * (repel + attract) - pEatCutoff_BB;
          //    fOverR = epsilon_BB * ((48.0 * repel) + attract)/rSquared;
              pe += epsilon_BB*((4.0 * (repel - attract)) - pEatCutoff);
              fOverR = epsilon_BB*24.0 * ((2.0 * repel) - attract)/rSquared;
            }
            else if ((atom_type[i]==0 && atom_type[j]==1) || (atom_type[i]==1 && atom_type[j]==0) ) 
            {
            //  pe += epsilon_AB * 4.0 * (repel - attract) - pEatCutoff_AB;
          //    fOverR = epsilon_AB* ((48 * repel) - attract)/rSquared;
              pe += epsilon_AB*((4.0 * (repel - attract)) - pEatCutoff);
              fOverR = epsilon_AB*24.0 * ((2.0 * repel) - attract)/rSquared;
            }  
            fx = fOverR * dx;
            fy = fOverR * dy;
            ax[i] += fx/250; // add this force on to i's acceleration (mass = 1)
            ay[i] += fy/250;
            ax[j] -= fx/250; // Newton's 3rd law
            ay[j] -= fy/250;
          }
        }
      }
    }
