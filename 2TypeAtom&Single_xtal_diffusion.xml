<Type>CODE_EDITOR</Type>
<Name>Initialize Molecules</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
add_Molecule_Rectangle ();
for (int i = 0; i < N; i++){
//  if (i<N_A) atom_type[i]=0;
  atom_type[i]=0;    
}

if(input>0)
{
  add_Molecule_Rectangle2();
}

for (int i = 0; i < N; i++)
{
  for (int j=0;j<N;j++) 
  {
    if (atom_type[i]==0 && atom_type[j]==0)
    {
      forceCutoff2_array[i][j]=sigma_AA*sigma_AA*forceCutoff2;
    }
    if ((atom_type[i]==0 && atom_type[j]==1)  || (atom_type[i]==1 && atom_type[j]==0))
    {
      forceCutoff2_array[i][j]=sigma_AB*sigma_AB*forceCutoff2;
    }
    if (atom_type[i]==1 && atom_type[j]==1)
    {
      forceCutoff2_array[i][j]=sigma_BB*sigma_BB*forceCutoff2;
    }
  }
}
pEatCutoff_AA=4*epsilon_AA*(Math.pow(sigma_AA/forceCutoff,12)-Math.pow(sigma_AA/forceCutoff,6));
pEatCutoff_BB=4*epsilon_BB*(Math.pow(sigma_BB/forceCutoff,12)-Math.pow(sigma_BB/forceCutoff,6));
pEatCutoff_AB=4*epsilon_AB*(Math.pow(sigma_AB/forceCutoff,12)-Math.pow(sigma_AB/forceCutoff,6));

computeAccelerations();
resetSums();

]]></Code>
