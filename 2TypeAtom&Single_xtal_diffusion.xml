<Osejs.Model.Evolution>
<Osejs.Model.Evolution.Page>
<Type>EVOLUTION_EDITOR</Type>
<Name>Verlet_Step</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
// Execute one time step using the Verlet algorithm (see Gould, Tobochnik, and Christian):
double dtOver2 = 0.5 * dt;
double dtSquaredOver2 = 0.5 * dt * dt;
for (int i = 0; i < N; i++) {
  x[i] += (vx[i] * dt) + (ax[i] * dtSquaredOver2);  // update position
  y[i] += (vy[i] * dt) + (ay[i] * dtSquaredOver2);
  vx[i] += (ax[i] * dtOver2);  // update velocity halfway
  vy[i] += (ay[i] * dtOver2);
}
computeAccelerations();
ke=0;
for (int i = 0; i < N; i++) {
  vx[i] += (ax[i] * dtOver2);  // finish updating velocity with new acceleration
  vy[i] += (ay[i] * dtOver2);
  ke+=0.5*(vx[i]*vx[i] + vy[i]*vy[i]);  // kinetic energy
}
t += dt;
count++;
sumPressure+=pressure;
sumTemperature+=ke/N;




]]></Code>
</Content>
</Osejs.Model.Evolution.Page>
