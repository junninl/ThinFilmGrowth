public void coolAction () {
  for (int i = 0; i < N; i++) {
//    if (atom_type[i]==0) {
      vx[i]/=10;
      vy[i]/=10;
  //    }
  }
  computeAccelerations();
  resetSums();
}

public void heatAction () {
  for (int i = 0; i < N; i++) {
    //if (atom_type2[i]==0) {
      vx[i]*=1.1;
      vy[i]*=1.1;
      //}
  }
  computeAccelerations();
  resetSums();
}

public void resetSums() {
  ke=0;
  for (int i = 0; i < N; i++) {
    ke+=0.5*(vx[i]*vx[i] + vy[i]*vy[i]);
  }
  sumTemperature=ke/N;  // initial values; update sums in evolution step
  sumPressure=pressure;
  count=1;
}
